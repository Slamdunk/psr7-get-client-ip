{
    "name": "slam/psr7-get-client-ip",
    "description": "Retrieve Client IP from a ServerRequestInterface",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.4.0 || ~8.5.0",
        "psr/http-message": "^1.1 || ^2.0"
    },
    "require-dev": {
        "infection/infection": "^0.32.3",
        "laminas/laminas-diactoros": "^3.8.0",
        "phpstan/phpstan": "^2.1.36",
        "phpstan/phpstan-phpunit": "^2.0.12",
        "phpunit/phpunit": "^12.5.6",
        "slam/php-cs-fixer-extensions": "^3.14.0"
    },
    "autoload": {
        "psr-4": {
            "Slam\\Psr7GetClientIp\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slam\\Psr7GetClientIp\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
