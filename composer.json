{
    "name": "slam/psr7-get-client-ip",
    "description": "Retrieve Client IP from a ServerRequestInterface",
    "license": "MIT",
    "authors": [
        {
            "name": "Filippo Tessarotto",
            "email": "zoeslam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.2.0 || ~8.3.0",
        "psr/http-message": "^1.1 || ^2.0"
    },
    "require-dev": {
        "infection/infection": "^0.29.6",
        "laminas/laminas-diactoros": "^3.4.0",
        "phpstan/phpstan": "^1.12.3",
        "phpstan/phpstan-phpunit": "^1.4.0",
        "phpunit/phpunit": "^11.3.4",
        "slam/php-cs-fixer-extensions": "^3.11.1"
    },
    "autoload": {
        "psr-4": {
            "Slam\\Psr7GetClientIp\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slam\\Psr7GetClientIp\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
